{
  "extends": ["airbnb-base", "prettier", "plugin:react/recommended"],
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "plugins": ["prettier"],
  "rules": {
    "no-mixed-operators": "off",
    "prettier/prettier": ["error"]
  },
  "settings": {
    "import/core-modules": "electron",
    "import/resolver": {
      "node": {
        "extensions": [".js",".jsx"]
      }
    }
  }
}
